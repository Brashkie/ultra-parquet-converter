  },
  "keywords": [
    "parquet",
    "converter",
    "csv",
    "xlsx",
    "json",
    "xml",
    "apache-arrow",
    "data-conversion",
    "cli",
    "bigdata",
    "python"
  ],
  "author": "Brashkie",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brashkie/ultra-parquet-converter.git"
  },
  "bugs": {
    "url": "https://github.com/Brashkie/ultra-parquet-converter/issues"
